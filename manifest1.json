{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": ["declarativeContent", "storage","activeTab"],
    "page_action": {
      "default_popup": "popup.html"
    },

    "manifest_version": 2,
    "content_scripts": [
   {
     "matches": ["https://www.facebook.com/*"],
     "js": ["contentScript.js"]
   }
 ]
  }
